package com.soda.entity;

import java.util.Date;

public class Employ {
	private Integer ey_id;
	private String ey_job;
	private Date ey_endtime;
	private Integer ey_number;
	private String ey_remark;
	private Integer ey_status;
	private String ey_other1;
	private String ey_other2;

	public Employ() {
		super();
	}

	public Employ(Integer ey_id, String ey_job, Date ey_endtime, Integer ey_number, String ey_remark, Integer ey_status, String ey_other1, String ey_other2) {
		this.ey_id = ey_id;
		this.ey_job = ey_job;
		this.ey_endtime = ey_endtime;
		this.ey_number = ey_number;
		this.ey_remark = ey_remark;
		this.ey_status = ey_status;
		this.ey_other1 = ey_other1;
		this.ey_other2 = ey_other2;
	}

	public void setEy_id(Integer ey_id) {
		this.ey_id = ey_id;
	}

	public Integer getEy_id(){
		return ey_id;
	}


	public void setEy_job(String ey_job) {
		this.ey_job = ey_job;
	}

	public String getEy_job(){
		return ey_job;
	}


	public void setEy_endtime(Date ey_endtime) {
		this.ey_endtime = ey_endtime;
	}

	public Date getEy_endtime(){
		return ey_endtime;
	}


	public void setEy_number(Integer ey_number) {
		this.ey_number = ey_number;
	}

	public Integer getEy_number(){
		return ey_number;
	}


	public void setEy_remark(String ey_remark) {
		this.ey_remark = ey_remark;
	}

	public String getEy_remark(){
		return ey_remark;
	}


	public void setEy_status(Integer ey_status) {
		this.ey_status = ey_status;
	}

	public Integer getEy_status(){
		return ey_status;
	}


	public void setEy_other1(String ey_other1) {
		this.ey_other1 = ey_other1;
	}

	public String getEy_other1(){
		return ey_other1;
	}


	public void setEy_other2(String ey_other2) {
		this.ey_other2 = ey_other2;
	}

	public String getEy_other2(){
		return ey_other2;
	}

}

