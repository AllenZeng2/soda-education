package com.soda.entity;

public class Stu_course {
	private Integer stuId;
	private Integer cId;
	private String ps;
	private String other2;

	public Stu_course() {
		super();
	}

	public Stu_course(Integer stuId, Integer cId, String ps, String other2) {
		this.stuId = stuId;
		this.cId = cId;
		this.ps = ps;
		this.other2 = other2;
	}

	public void setStuId(Integer stuId) {
		this.stuId = stuId;
	}

	public Integer getStuId(){
		return stuId;
	}


	public void setCId(Integer cId) {
		this.cId = cId;
	}

	public Integer getCId(){
		return cId;
	}


	public void setPs(String ps) {
		this.ps = ps;
	}

	public String getPs(){
		return ps;
	}


	public void setOther2(String other2) {
		this.other2 = other2;
	}

	public String getOther2(){
		return other2;
	}

}

